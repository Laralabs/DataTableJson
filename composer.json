{
    "name": "laralabs/datatablejson",
    "description": "Easily generate DataTable JSON from an eloquent collection and bind it to the view",
    "license": "MIT License",
    "authors": [
        {
            "name": "Matt Clinton",
            "email": "matt@laralabs.uk"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "illuminate/support": "5.4.*|5.5.*",
        "illuminate/database": "5.4.*|5.5.*",
        "illuminate/contracts": "5.4.*|5.5.*"
    },
    "autoload": {
        "psr-4": {
            "Laralabs\\DataTableJson\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laralabs\\DataTableJson\\DataTableJsonServiceProvider"
            ],
            "aliases": {
                "DataTableJsonFacade": "Laralabs\\DataTableJson\\Facades\\DataTableJsonFacade"
            }
        }
    }
}
